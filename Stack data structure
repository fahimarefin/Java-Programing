import java.util.ArrayList;
import java.util.Scanner;

public class Main {

    public static void main(String[] args) {
        Scanner MyScanner=new Scanner(System.in);
       MyStack myStack=new MyStack();
        System.out.println("Enter five numbers:");
        for(int i=0; i<5; i++){
            myStack.push(MyScanner.next());
        }
        System.out.println("Is empty?"+myStack.isEmpty());
        System.out.println("Give the size of the array list:"+myStack.getSize());
        System.out.println("Return the top element from the array:"+myStack.peek());
        System.out.println("Returns and remove the top elements from the array:"+myStack.pop());
        myStack.push(20);
        System.out.println("My Stack:"+myStack.toString());

    }
}
class MyStack{
    ArrayList<Object>list=new ArrayList<Object>();
    //Return true if the list is empty
    public boolean isEmpty(){
        return list.isEmpty();
    }
    //Return the size of the array
    public int getSize(){
        return list.size();
    }
    //Return the top element of the array
    public Object peek(){
        return list.get(getSize()-1);
    }
    //Returns and remove the top element from the array
    public Object pop(){
        Object o=list.get(getSize()-1);
        list.remove(getSize()-1);
        return o;
    }
    //Add an element in the array
    public void push(Object o){
        list.add(o);
    }

    @Override
    public String toString() {
        return "MyStack{" +
                "list=" + list +
                '}';
    }
}
